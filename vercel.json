{
  "version": 2,
  "routes": [
    {
      "src": "/",
      "status": 308,
      "headers": {
        "Location": "/marketing-app"
      }
    },
    {
      "src": "/marketing-app/(.*)",
      "dest": "/marketing-app/$1"
    },
    {
      "src": "/marketing-app/api/auth/(.*)",
      "dest": "/api/auth/index.js"
    }
  ]
}
