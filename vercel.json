{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/auth/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/progress/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/index.js"
    },
    {
      "src": "/api/progress/(.*)",
      "dest": "/api/progress/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}
